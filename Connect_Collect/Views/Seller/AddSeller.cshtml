@{
}
@model Connect_Collect.Models.AddSellerViewModel

<h1>Seller Signup</h1>
<form method="post">
    <div class="mt-3">
        <label class="form-label"> SellerName</label>
        <input type="text" class="form-control" asp-for="SellerName" />
    </div>
    <div class="mt-3">
        <label class="form-label"> SellerId</label>
        <input type="text" class="form-control" asp-for="SellerId" />
    </div>
    <div class="mt-3">
        <label class="form-label"> Email</label>
        <input type="text" class="form-control" asp-for="Email" />
    </div>
    <div class="mt-3">
        <label class="form-label"> Password</label>
        <input type="password" class="form-control" asp-for="Password" />
    </div>
    <div class="mt-3">
        <label class="form-label"> Contact </label>
        <input type="text" class="form-control" asp-for="Contact" />
    </div>
    @* <div class="mt-3">
    <input type="checkbox" class="form-check-input" id="finish" asp-for="finish"/>
    <label class="form-check-label" for="finish"> Finish </label>
    </div> *@
    <div class="mt-3">
        <button type="submit" class="btn btn-primary"> SAVE </button>
    </div>
</form>