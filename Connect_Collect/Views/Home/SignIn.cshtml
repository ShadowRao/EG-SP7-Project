@{
}
@model Connect_Collect.Models.SignInModel
<link rel="stylesheet" href="~/css/CommonStyle.css" />
<h1> SignIn </h1>
<form method="get" asp-action="SignIn" asp-controller="Home">
    <div class="mt-3">
        <label class="form-label"> Email</label>
        <input type="text" class="form-control" asp-for="Email" required/>
    </div>
    <div class="mt-3">
        <label class="form-label"> Password</label>
        <input type="password" class="form-control" asp-for="Password" required />
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary"> Sign In </button>
    </div>
</form>